<ul>
	<% unless @unread_notifications.nil? %>
	<% @unread_notifications.each do |notification| %>

		<li>
			<p class="unread_notification"><%= link_to  notification.to_s, notification.notification_object %></p>
		</li>

	<% end %>
	<% end %>
	<% unless @read_notifications.nil? %>
	<% @read_notifications.each do |notification| %>

		<li>
			<p class="read_notification">%= link_to  notification.to_s, notification.notification_object %></p>
		</li>

	<% end %>
	<% end %>
</ul>

